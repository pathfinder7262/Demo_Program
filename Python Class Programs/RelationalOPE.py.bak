#5 . which demonstrate a concept of relational operaters.

a = int(input("Enter the value for A\t"))
b = int(input("Enter the value for B\t"))

#apply the relational operater

print("RESULT:-->")
print("{}>{}={}\n".format(a,b,a>b))
print("{}<{}={}\n".format(a,b,a<b))
print("{}>={}={}\n".format(a,b,a>=b))
print("{}<={}={}\n".format(a,b,a<=b))
print("{}=={}={}\n".format(a,b,a==b))
print("{}!={}={}\n".format(a,b,a!=b))